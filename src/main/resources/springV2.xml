<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
 		http://www.springframework.org/schema/beans
 		http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean id="client" class="by.vabramov.spring.core.course.v2.ClientV2">
        <constructor-arg value="1"/>
        <constructor-arg value="John Smith"/>
    </bean>

    <bean id="eventLogger" class="by.vabramov.spring.core.course.v2.ConsoleEventLoggerV2"/>

    <bean id="app" class="by.vabramov.spring.core.course.v2.AppV2">
        <constructor-arg ref="client"/>
        <constructor-arg ref="eventLogger"/>
    </bean>

</beans>